{
  "manifest_version": 2,

  "name": "IP Address Viewer",
  "description": "Displays your public IP address and the country that it belongs to.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "IP Address Viewer"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://tools.keycdn.com/"
  ],
  "web_accessible_resources":[
    "images/*"
  ]
}